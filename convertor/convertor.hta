<HTA:APPLICATION
     ID="convertor"
     APPLICATIONNAME="ConvertorTest"
     SCROLL="no"
     SINGLEINSTANCE="yes"
     #CAPTION="no"
     #BORDER="none"
     #CONTEXTMENU="no"
     INNERBORDER="no"
     #ICON="C:\Program Files\Microsoft Office\OFFICE11\MSN.ICO"
